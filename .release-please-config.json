{
  "release-type": "node",
  "packages": {
    ".": {
      "package-name": "nuvio-streams-addon",
      "changelog-sections": [
        {"type": "feat", "section": "Features"},
        {"type": "fix", "section": "Bug Fixes"},
        {"type": "perf", "section": "Performance Improvements"},
        {"type": "deps", "section": "Dependencies"},
        {"type": "chore", "section": "Miscellaneous", "hidden": true},
        {"type": "docs", "section": "Documentation", "hidden": true},
        {"type": "style", "section": "Styles", "hidden": true},
        {"type": "refactor", "section": "Code Refactoring", "hidden": true},
        {"type": "test", "section": "Tests", "hidden": true},
        {"type": "build", "section": "Build System", "hidden": true},
        {"type": "ci", "section": "Continuous Integration", "hidden": true}
      ],
      "extra-files": [
        {
          "type": "json",
          "path": "manifest.json",
          "jsonpath": "$.version"
        },
        {
          "type": "json",
          "path": "provider-service/package.json",
          "jsonpath": "$.version"
        },
        {
          "type": "json",
          "path": "providers/hianime/package.json",
          "jsonpath": "$.version"
        }
      ]
    }
  },
  "bump-minor-pre-major": true,
  "bump-patch-for-minor-pre-major": true,
  "draft": false,
  "prerelease": false,
  "include-v-in-tag": true,
  "versioning-strategy": "default",
  "changelog-type": "default",
  "pull-request-title-pattern": "chore(main): release ${version}",
  "pull-request-header": "This PR was generated by Release Please. See [documentation](https://github.com/googleapis/release-please).",
  "separate-pull-requests": false,
  "tag-separator": "-",
  "include-component-in-tag": false,
  "release-search-depth": 400,
  "commit-search-depth": 500
}